{
  "name": "price-request-app",
  "version": "2.1.0",
  "description": "Application NestJS automatisée pour générer des demandes de prix à partir d'emails (PDF, Excel, Word) avec support OCR et révision manuelle",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop price-request-generator",
    "pm2:restart": "pm2 restart price-request-generator",
    "pm2:logs": "pm2 logs price-request-generator",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "keywords": [
    "nestjs",
    "email",
    "pdf",
    "excel",
    "price-request",
    "ocr",
    "manual-review"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/serve-static": "^5.0.1",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cron": "^4.4.0",
    "exceljs": "^4.4.0",
    "fast-glob": "^3.3.3",
    "imap-simple": "^5.1.0",
    "mailparser": "^3.9.1",
    "mammoth": "^1.11.0",
    "nodemailer": "^7.0.12",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.530",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sharp": "^0.34.5",
    "sql.js": "^1.13.0",
    "tesseract.js": "^7.0.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/testing": "^11.1.11",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
